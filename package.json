{
  "name": "superapp",
  "version": "0.1.0",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://git.coding.net/lipengpeng/superapp.git"
  },
  "author": "lipengpeng",
  "babel": {
    "presets": "env"
  },
  "scripts": {
    "test": "jest --coverage --no-cache",
    "build": "npm run bundle && npm run minify",
    "bundle": "rollup -i src/index.js -o dist/superapp.js -m -f umd -n superapp",
    "minify": "uglifyjs dist/superapp.js -o dist/superapp.min.js -mc pure_funcs=Object.defineProperty --source-map includeSources,url=superapp.js.map"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "jest": "^22.0.4",
    "prettier": "^1.9.2",
    "rollup": "^0.53.0",
    "typescript": "^2.6.2",
    "uglify-es": "^3.3.9"
  }
}